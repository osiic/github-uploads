# GitHub Personal Access Token dengan akses repo
GITHUB_TOKEN=your_github_token_here

# Username atau organisasi GitHub pemilik repo
OWNER=your_github_username_or_org

# Nama repository GitHub tempat file diupload
REPO=your_repo_name

# Nama branch pada repo (biasanya main atau master)
BRANCH=main

# Port tempat server Bun berjalan (default 3000)
PORT=3000
